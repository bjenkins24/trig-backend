{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "ext-dom": "*",
        "ext-json": "*",
        "ext-sockets": "*",
        "andreskrey/readability.php": "^2.1",
        "aws/aws-sdk-php": "^3.150",
        "babenkoivan/elastic-migrations": "^1.3",
        "babenkoivan/elastic-scout-driver": "^1.2",
        "babenkoivan/elastic-scout-driver-plus": "^1.14",
        "campo/random-user-agent": "^1.3",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "google/apiclient": "^2.4",
        "guzzlehttp/guzzle": "^6.3",
        "html2text/html2text": "^4.3",
        "intervention/image": "^2.5",
        "laravel/framework": "^7.0",
        "laravel/horizon": "^4.3",
        "laravel/nova": "*",
        "laravel/passport": "^8.4",
        "laravel/scout": "^8.0",
        "laravel/tinker": "^2.0",
        "lcobucci/jwt": "3.3.3",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-cached-adapter": "^1.0",
        "league/html-to-markdown": "^4.10",
        "league/flysystem": "^1.1",
        "mews/purifier": "^3.3",
        "nesk/puphpeteer": "^1.6",
        "sentry/sentry-laravel": "^1.8",
        "vaites/php-apache-tika": "^0.9.2"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "barryvdh/laravel-ide-helper": "^2.7",
        "facade/ignition": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "morrislaptop/laravel-queue-clear": "^1.2",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "rregeer/phpunit-coverage-check": "^0.3.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "disable-tls": false
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    }
}
